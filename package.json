{
  "name": "music-player",
  "version": "2.0.2",
  "description": "Play your music in the tray.",
  "homepage": "https://github.com/tghgg/music-player",
  "main": "main.js",
  "scripts": {
    "start": "electron --disable-gpu .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist_mac": "electron-builder -m",
    "dist_windows": "electron-builder -w",
    "dist_linux": "electron-builder -l"
  },
  "build": {
    "appId": "com.musicplayer.app",
    "productName": "Music Player",
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "AudioVideo"
    },
    "win": {
      "target": "NSIS"
    },
    "mac": {
      "target": "default",
      "category": "public.app-category.music"
    }
  },
  "author": {
    "name": "Choppa2",
    "email": "falling_snowdin@protonmail.com"
  },
  "repository": "https://github.com/tghgg/music-player.git",
  "license": "MIT",
  "devDependencies": {
    "electron": "^7.1.11",
    "electron-builder": "^22.3.2"
  },
  "dependencies": {
    "electron-store": "^5.1.0"
  }
}
